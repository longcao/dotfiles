{
  "editor.fontFamily": "Inconsolata",
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [120],
  "editor.tabSize": 2,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.bloop": true,
    "**/.metals": true,
    "**/.ammonite": true
  },
  "javascript.format.enable": false,
  "bazel.enableCodeLens": true,
  "puppet.titleBar.pdkNewModule.enable": false,
  "python.formatting.blackPath": "~/.pyenv/shims/black",
  "python.formatting.provider": "black",
  "python.languageServer": "Pylance",
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Path": "~/.pyenv/shims/flake8",
  "python.linting.mypyEnabled": true,
  "python.linting.mypyPath": "~/.pyenv/shims/mypy",
  "python.linting.pylintEnabled": false,
  "terminal.integrated.shell.osx": "/usr/local/bin/fish",
  "vim.hlsearch": true,
  "vim.leader": ",",
  "vim.useSystemClipboard": true,
  "window.newWindowDimensions": "maximized",
  "window.restoreWindows": "none",
  "workbench.colorCustomizations": {
    "[Dark (Molokai)]": {
      "statusBar.background": "#5c5c5c",
      "statusBar.noFolderBackground": "#5c5c5c"
    }
  },
  "workbench.colorTheme": "Dark (Molokai)",
  "workbench.editor.enablePreview": false,
  "workbench.startupEditor": "none",
}
